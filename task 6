a = int(input())
if a<20:
    if a==1:
        print(a,'программист')
    elif 1<a<5:
        print(a, 'программиста')
    else:
        print(a, 'программистов')
elif a<100:
    n =a%10
    if n==1:
        print(a,'программист')
    elif 1<n<5:
        print(a, 'программиста')
    else:
        print(a, 'программистов')
elif a<1001:
    n =a % 100 % 10
    if n==1:
        print(a,'программист')
    elif 1<n<5:
        print(a, 'программиста')
    else:
        print(a, 'программистов')
